im","level":5},{"line":85,"heading":"Related","level":2},{"line":93,"heading":"Maintainers","level":2},{"line":100,"heading":"Security","level":2},{"line":105,"heading":"License","level":2}]},"0d2537e86a9e5ae85d8e53910d779895179035c9daa35555874ec19f1225c224":{"links":[],"embeds":[],"tags":[],"headings":[{"line":0,"heading":"wrappy","level":1},{"line":4,"heading":"USAGE","level":2}]},"dc0df537682fe3be61af2ba199cc82e809536d940d388570cb5ceeeba7f3af31":{"links":[{"line":23,"link":"#protocol-support","original":"[Protocol support](#protocol-support)","displayText":"Protocol support","beforeContext":"","afterContext":""},{"line":24,"link":"#installing","original":"[Installing](#installing)","displayText":"Installing","beforeContext":"","afterContext":""},{"line":25,"link":"#opt-in-for-performance-and-spec-compliance","original":"[Opt-in for performance and spec compliance](#opt-in-for-performance-and-spec-compliance)","displayText":"Opt-in for performance and spec compliance","beforeContext":"","afterContext":""},{"line":26,"link":"#api-docs","original":"[API docs](#api-docs)","displayText":"API docs","beforeContext":"","afterContext":""},{"line":27,"link":"#websocket-compression","original":"[WebSocket compression](#websocket-compression)","displayText":"WebSocket compression","beforeContext":"","afterContext":""},{"line":28,"link":"#usage-examples","original":"[Usage examples](#usage-examples)","displayText":"Usage examples","beforeContext":"","afterContext":""},{"line":29,"link":"#sending-and-receiving-text-data","original":"[Sending and receiving text data](#sending-and-receiving-text-data)","displayText":"Sending and receiving text data","beforeContext":"","afterContext":""},{"line":30,"link":"#sending-binary-data","original":"[Sending binary data](#sending-binary-data)","displayText":"Sending binary data","beforeContext":"","afterContext":""},{"line":31,"link":"#simple-server","original":"[Simple server](#simple-server)","displayText":"Simple server","beforeContext"